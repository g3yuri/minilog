{
  "name": "@g3yuri/minilog",
  "version": "0.1.1",
  "description": "Una librer√≠a JavaScript simple para registrar eventos y acciones de aplicaciones",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=node14 --outfile=dist/cjs/index.js --format=cjs",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --target=es2020 --outfile=dist/esm/index.js --format=esm",
    "build:types": "tsc --declaration --declarationDir dist/types --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "logging",
    "events",
    "analytics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/g3yuri/minilog.git"
  },
  "author": "Yuri Quispe",
  "license": "CC-BY-NC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "hono": "^4.7.10",
    "uuid": "^11.1.0"
  }
}
